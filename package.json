{
  "name": "uniq-rng",
  "version": "0.0.1",
  "description": "Unique Random Number Generator (using ES2016 generator)",
  "main": "lib/index.js",
  "scripts": {
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint ./src --cache",
    "lint:test": "eslint ./test --cache",
    "test": "tape -r babel-register test/index.js | tap-spec",
    "test:watch": "tape-watch -r babel-register -p tap-spec test/index.js",
    "build": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m4n3z40/uniq-rng.git"
  },
  "keywords": [
    "unique",
    "random",
    "rng",
    "number",
    "generator",
    "ES2016"
  ],
  "author": "Allan Baptista (allan.marques@ymail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m4n3z40/uniq-rng/issues"
  },
  "homepage": "https://github.com/m4n3z40/uniq-rng#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.12.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-watch": "^2.1.0"
  }
}
